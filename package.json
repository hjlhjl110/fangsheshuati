    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
